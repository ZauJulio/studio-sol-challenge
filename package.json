{
  "name": "studio-sol-challenge",
  "version": "0.1.0",
  "description": "Studio Sol Comunicação Challenge 2023/01",
  "main": "src/main.ts",
  "repository": "git@github.com:ZauJulio/studio-sol-challenge.git",
  "author": "zaujulio.dev@gmail.com",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsup",
    "start": "node dist/main.js",
    "dev": "DEBUG=* tsx watch src/main.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "scan": "./scripts/sonar-scanner.sh",
    "docs:gen": "graphdoc --force",
    "prepare": "husky install"
  },
  "tsup": {
    "entry": [
      "src/main.ts"
    ],
    "splitting": false,
    "clean": true
  },
  "graphdoc": {
    "endpoint": "http://localhost:8080/graphql",
    "output": "./public/docs/graphql"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test && npm run coverage"
    }
  },
  "dependencies": {
    "@apollo/server": "^4.3.1",
    "@graphql-tools/utils": "^9.1.4",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-winston": "^4.2.0",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "swagger-ui-express": "^4.6.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitest/coverage-c8": "^0.28.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-commit-msg-linter": "^4.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "tsup": "^6.5.0",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "vitest": "^0.28.1"
  }
}